#!/usr/bin/gnuplot
reset
set terminal png
set output "images/thread.png"
set boxwidth 0.75 absolute
set style fill solid 1.00 border -1
set style histogram rowstacked
set style data histograms
set xtics 1.0
set mytics 1.0
set ytics 1.0
set yr [0.0:13.0]
set ylabel "TIME"
set xlabel "VERSION"
set grid
plot 'data/thread.dat' using 2:xtic(1) t "THREAD"
